   {
     "extends": "expo/tsconfig.base",
     "compilerOptions": {
       "strict": true,
       "baseUrl": ".",
       "paths": {
         "src/*": ["src/*"],
         "api/*": ["src/api/*"],
         "common/*": ["src/common/*"],
         "models/*": ["src/models/*"]
       },
       "typeRoots": ["./src/types", "./node_modules/@types"]
     },
     "include": ["src/**/*"]
   }