{
    "expo": {
        "name": "Dygat",
        "slug": "dygat",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/dygat.png",
        "userInterfaceStyle": "dark",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/dygat.png",
            "resizeMode": "contain",
            "backgroundColor": "#011627"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.dangervalentine.dygat",
            "adaptiveIcon": {
                "foregroundImage": "./assets/dygat.png",
                "backgroundColor": "#011627"
            }
        },
        "web": {
            "favicon": "./assets/dygat.png"
        },
        "plugins": [
            "expo-sqlite",
            "expo-font"
        ]
    }
}